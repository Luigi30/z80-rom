      47.  00:9805                      
      48.  00:9805  (00:0040)           tmswritebit:    equ $40                 ; bit to indicate memory write
      49.  00:9805  (00:0080)           tmsregbit:      equ $80                 ; bit to indicate register write
      50.  00:9805                      
      51.  00:9805  (00:0000)           tmsctrl0:       equ 0                   ; control bits
      52.  00:9805  (00:0001)           tmsmode3:       equ 1                   ;       mode bit 3
      53.  00:9805  (00:0000)           tmsextvid:      equ 0                   ;       external video
      54.  00:9805                      
      55.  00:9805  (00:0001)           tmsctrl1:       equ 1                   ; control bits
      56.  00:9805  (00:0007)           tms4k16k:       equ 7                   ;       4/16K RAM
      57.  00:9805  (00:0006)           tmsblank:       equ 6                   ;       screen blank
      58.  00:9805  (00:0005)           tmsinten:       equ 5                   ;       interrupt enable
      59.  00:9805  (00:0004)           tmsmode1:       equ 4                   ;       mode bit 1
      60.  00:9805  (00:0003)           tmsmode2:       equ 3                   ;       mode bit 2
      61.  00:9805  (00:0001)           tmssprsize:     equ 1                   ;       sprite size
      62.  00:9805  (00:0000)           tmssprmag:      equ 0                   ;       sprite magnification
      63.  00:9805                      
      64.  00:9805  (00:0002)           tmsnametbl:     equ 2                   ; name table location (* $400)
      65.  00:9805  (00:0003)           tmscolortbl:    equ 3                   ; color table location (* $40)
      66.  00:9805                                                              ;       graphics 2 mode: MSB 0 = $0000, MSB 1 = $2000
      67.  00:9805  (00:0004)           tmspattern:     equ 4                   ; pattern table location (* $800)
      68.  00:9805  (00:0005)           tmsspriteattr:  equ 5                   ; sprite attribute table (* $80)
      69.  00:9805  (00:0006)           tmsspritepttn:  equ 6                   ; sprite pattern table (* $800)
      70.  00:9805  (00:0007)           tmscolor:       equ 7                   ; screen colors (upper = text, lower = background)
