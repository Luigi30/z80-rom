      93.  00:9805                      ; register configuration routines
      94.  00:9805                      
      95.  00:9805                      ; shadow copy of register values
      96.  00:9805                      tmsshadow:
      97.  00:9805  00 (8)                      defs    8, 0
      98.  00:980D                      
      99.  00:980D                      ; set a single register value
     100.  00:980D                      ;       A = register value
     101.  00:980D                      ;       E = register to set
     102.  00:980D                      tmssetreg:
     103.  00:980D  21 05 98                    ld      hl, tmsshadow           ; get shadow table address
     104.  00:9810  16 00                       ld      d, 0
     105.  00:9812  19                          add     hl, de                  ; add offset to selected register
     106.  00:9813  77                          ld      (hl), a                 ; save to shadow slot
     107.  00:9814  D3 BF                       out     (tmsreg), a             ; send to TMS
     108.  00:9816  3E 80                       ld      a, tmsregbit            ; select requested register
     109.  00:9818  B3                          or      e
     110.  00:9819  D3 BF                       out     (tmsreg), a
     111.  00:981B  C9                          ret
     112.  00:981C                      
     113.  00:981C                      ; set the background color
     114.  00:981C                      ;       A = requested color
     115.  00:981C                      tmsbackground:
     116.  00:981C  E6 0F                       and     $0F                     ; mask off high nybble
     117.  00:981E  47                          ld      b, a                    ; save for later
     118.  00:981F  3A 0C 98                    ld      a, (tmsshadow+tmscolor) ; get current colors
     119.  00:9822  E6 F0                       and     $F0                     ; mask off old background
     120.  00:9824  B0                          or      b                       ; set new background
     121.  00:9825  1E 07                       ld      e, tmscolor
     122.  00:9827  C3 0D 98                    jp      tmssetreg               ; set the color
     123.  00:982A                      
     124.  00:982A                      ; enable vblank interrupts
     125.  00:982A                      tmsintenable:
     126.  00:982A  3A 06 98                    ld      a, (tmsshadow+tmsctrl1) ; get current control register value
     127.  00:982D  CB EF                       set     tmsinten, a             ; set interrupt enable bit
     128.  00:982F  1E 01                       ld      e, tmsctrl1
     129.  00:9831  C3 0D 98                    jp      tmssetreg               ; save it back
     130.  00:9834                      
     131.  00:9834                      ; disable vblank interrupts
     132.  00:9834                      tmsintdisable:
     133.  00:9834  3A 06 98                    ld      a, (tmsshadow+tmsctrl1) ; get current control register value
     134.  00:9837  CB AF                       res     tmsinten, a             ; clear interrupt enable bit
     135.  00:9839  1E 01                       ld      e, tmsctrl1
     136.  00:983B  C3 0D 98                    jp      tmssetreg               ; save it back
     137.  00:983E                      
     138.  00:983E                      ; configure tms from specified register table
     139.  00:983E                      ;       HL = register table
     140.  00:983E                      tmsconfig:
     141.  00:983E  11 05 98                    ld      de, tmsshadow           ; start of shadow area
     142.  00:9841  0E 08               	ld      c, 8                    ; 8 registers
     143.  00:9843                      regloop:
     144.  00:9843  7E                    	ld      a, (hl)                 ; get register value from table
     145.  00:9844  D3 BF               	out     (tmsreg), a             ; send it to the TMS
     146.  00:9846  3E 08               	ld      a, 8                    ; calculate current register number
     147.  00:9848  91                  	sub     c
     148.  00:9849  F6 80               	or      tmsregbit               ; set high bit to indicate a register
     149.  00:984B  ED A0                       ldi                             ; shadow, then inc pointers and dec counter
     150.  00:984D  D3 BF               	out     (tmsreg), a             ; send it to the TMS
     151.  00:984F  AF                          xor     a                       ; continue until count reaches 0
     152.  00:9850  B1                          or      c
     153.  00:9851  20 F0               	jr      nz, regloop
     154.  00:9853  C9                  	ret
     155.  00:9854                      
     156.  00:9854                      ; ---------------------------------------------------------------------------
     157.  00:9854                      ; memory access routines
     158.  00:9854                      
     159.  00:9854                      ; set the next address of vram to write
     160.  00:9854                      ;       DE = address
     161.  00:9854                      tmswriteaddr:
     162.  00:9854  7B                          ld      a, e                    ; send lsb
     163.  00:9855  D3 BF                       out     (tmsreg), a
     164.  00:9857  7A                          ld      a, d                    ; mask off msb to max of 16KB
     165.  00:9858  E6 3F                       and     $3F
     166.  00:985A  F6 40                       or      $40                     ; set second highest bit to indicate write
     167.  00:985C  D3 BF                       out     (tmsreg), a             ; send msb
     168.  00:985E  C9                          ret
     169.  00:985F                      
     170.  00:985F                      ; set the next address of vram to read
     171.  00:985F                      ;       DE = address
     172.  00:985F                      tmsreadaddr:
     173.  00:985F  7B                          ld      a, e                    ; send lsb
     174.  00:9860  D3 BF                       out     (tmsreg), a
     175.  00:9862  7A                          ld      a, d                    ; mask off msb to max of 16KB
     176.  00:9863  E6 3F                       and     $3F
     177.  00:9865  D3 BF                       out     (tmsreg), a             ; send msb
     178.  00:9867  C9                          ret
     179.  00:9868                      
     180.  00:9868                      ; copy bytes from ram to vram
     181.  00:9868                      ;       HL = ram source address
     182.  00:9868                      ;       DE = vram destination address
     183.  00:9868                      ;       BC = byte count
     184.  00:9868                      tmswrite:
     185.  00:9868  CD 54 98                    call    tmswriteaddr            ; set the starting address
     186.  00:986B                      copyloop:
     187.  00:986B  7E                          ld      a, (hl)                 ; get the current byte from ram
     188.  00:986C  D3 BE                       out     (tmsram), a             ; send it to vram
     189.  00:986E  00 (11)                     defs    11/tmswait, 0         ; nops to waste time
     190.  00:9879  23                          inc     hl                      ; next byte
     191.  00:987A  0B                          dec     bc                      ; continue until count is zero
     192.  00:987B  78                          ld      a, b
     193.  00:987C  B1                          or      c
     194.  00:987D  20 EC                       jr      nz, copyloop
     195.  00:987F  C9                          ret
     196.  00:9880                      
     197.  00:9880                      ; fill a section of memory with a single value
     198.  00:9880                      ;       A = value to fill
     199.  00:9880                      ;       DE = vram destination address
     200.  00:9880                      ;       BC = byte count
     201.  00:9880                      tmsfill:
     202.  00:9880  F5                          push    af
     203.  00:9881  CD 54 98                    call    tmswriteaddr            ; set the starting address
     204.  00:9884  F1                          pop     af
     205.  00:9885                      fillloop:
     206.  00:9885  D3 BE                       out     (tmsram), a             ; send it to vram
     207.  00:9887  00 (11)                     defs    11/tmswait, 0           ; nops to waste time
     208.  00:9892  0D                          dec     c
     209.  00:9893  C2 85 98                    jp      nz, fillloop
     210.  00:9896  10 ED                       djnz    fillloop                ; continue until count is zero
