      14.  01:020C                      ;;; ;;;;;;;;;;;;;;;;;
      15.  01:020C                      ;;; VDP functions
      16.  01:020C                      ;;;
      17.  01:020C                      ;;; API:
      18.  01:020C                      ;;;		All VDP BIOS functions are prefixed with VDP_B_
      19.  01:020C                      ;;; 	Input is A, BC, DE, HL
      20.  01:020C                      ;;;		Output bytes are in A
      21.  01:020C                      ;;;		Output words are in HL (todo: ?)
      22.  01:020C                      ;;;
      23.  01:020C                      ;;;		Do not assume any registers are preserved.
      24.  01:020C                      VDP_B_Dispatch:
      25.  01:020C                      	;; Dispatch to the function number C.
      26.  01:020C  08                      ex      af
      27.  01:020D  FD 69                   ld      iyl,c
      28.  01:020F  D9                      exx
      29.  01:0210  FD 4D                   ld      c,iyl
      30.  01:0212  21 00 84            	ld		hl,VDP_FnTable_Public	; grab the jump table address
      31.  01:0215  16 00               	ld		d,0		; clear D
      32.  01:0217                      	
      33.  01:0217  CB 21               	sla		c		; shift C to produce a table offset
      34.  01:0219  59                  	ld		e,c		; E <- C
      35.  01:021A  19                  	add		hl,de	; Apply the offset.
      36.  01:021B                      
      37.  01:021B  7E                  	ld		a,(hl)	; Get the destination address. 
      38.  01:021C  5F                  	ld		e,a
      39.  01:021D  23                  	inc		hl
      40.  01:021E  56                  	ld		d,(hl)
      41.  01:021F                      
      42.  01:021F  D5                  	push    de
      43.  01:0220  DD E1                   pop     ix
      44.  01:0222                      
      45.  01:0222  D9                      exx
      46.  01:0223  08                      ex      af
      47.  01:0224  DD E9               	jp		(ix)	; Jump to the BIOS function, which RETs back to where we started.
      48.  01:0226  C9                  	ret				; Unnecessary unless something breaks
      49.  01:0227                      
      50.  01:0227                      ; copy a null-terminated string to VRAM
      51.  01:0227                      ;       HL = ram source address
      52.  01:0227                      VDP_B_StringOut:
      53.  01:0227  7E                      ld      a, (hl)                 ; get the current byte from ram
      54.  01:0228  FE 00                   cp      0                       ; return when NULL is encountered
      55.  01:022A  C8                      ret     z
      56.  01:022B  D3 98                   out     (VDP_PORT_VRAM), a             ; send it to vram
      57.  01:022D  00 (3)                  defs    14/tmswait, 0         ; nops to waste time
      58.  01:0230  23                      inc     hl                      ; next byte
      59.  01:0231  18 F4                   jr      VDP_B_StringOut
      60.  01:0233                      
      61.  01:0233                      VDP_B_VRAMBlockCopy:
      62.  01:0233                      ; copy bytes from ram to vram
      63.  01:0233                      ;       HL = ram source address
      64.  01:0233                      ;       DE = vram destination address
      65.  01:0233                      ;       BC = byte count
      66.  01:0233  CD 68 02                call    VDP_B_SetVRAMAddress      ; set the starting address
      67.  01:0236                      .copyloop:
      68.  01:0236  7E                      ld      a, (hl)                 ; get the current byte from ram
      69.  01:0237  D3 98                   out     (VDP_PORT_VRAM), a           ; send it to vram
      70.  01:0239  00 (3)                  defs    14/tmswait, 0         ; nops to waste time
      71.  01:023C  23                      inc     hl                      ; next byte
      72.  01:023D  0B                      dec     bc                      ; continue until count is zero
      73.  01:023E  78                      ld      a, b
      74.  01:023F  B1                      or      c
      75.  01:0240  20 F4                   jr      nz, .copyloop
      76.  01:0242  C9                      ret
      77.  01:0243                      
      78.  01:0243                      VDP_B_SetRegister:
      79.  01:0243                          ; A is the value
      80.  01:0243                          ; E is the register number
      81.  01:0243                      
      82.  01:0243  21 72 82                ld      hl,VDP_ShadowRegs
      83.  01:0246  16 00                   ld      d,0
      84.  01:0248  19                      add     hl,de   ; Add offset to shadow register.
      85.  01:0249  77                      ld      (hl),a  ; Update shadow value.
      86.  01:024A  D3 99                   out     (VDP_PORT_REGS),a    ; send a byte of data
      87.  01:024C  3E 80                   ld      a,VDP_REG_WRITE ; select register to write it to
      88.  01:024E  B3                      or      e
      89.  01:024F  D3 99                   out     (VDP_PORT_REGS),a
      90.  01:0251                      
      91.  01:0251  C9                      ret
      92.  01:0252                      
      93.  01:0252                      VDP_B_SetRegistersFromArray:
      94.  01:0252                          ; HL is an array of registers.
      95.  01:0252  11 72 82                ld      de, VDP_ShadowRegs      ; start of shadow area
      96.  01:0255  0E 08               	ld      c, 8                    ; 8 registers
      97.  01:0257                      .regloop:
      98.  01:0257  7E                    	ld      a, (hl)                 ; get register value from table
      99.  01:0258  D3 99               	out     (VDP_PORT_REGS), a      ; send it to the TMS
     100.  01:025A  3E 08               	ld      a, 8                    ; calculate current register number
     101.  01:025C  91                  	sub     c
     102.  01:025D  F6 80               	or      VDP_REG_WRITE           ; set high bit to indicate a register
     103.  01:025F  ED A0                   ldi                             ; shadow, then inc pointers and dec counter
     104.  01:0261  D3 99               	out     (VDP_PORT_REGS), a           ; send it to the TMS
     105.  01:0263  AF                      xor     a                       ; continue until count reaches 0
     106.  01:0264  B1                      or      c
     107.  01:0265  20 F0               	jr      nz, .regloop
     108.  01:0267  C9                  	ret
     109.  01:0268                      
     110.  01:0268                      ; set the next address of vram to write
     111.  01:0268                      ;   DE = address
     112.  01:0268                      VDP_B_SetVRAMAddress:
     113.  01:0268  7B                      ld      a, e                    ; send lsb
     114.  01:0269  D3 99                   out     (VDP_PORT_REGS), a
     115.  01:026B  7A                      ld      a, d                    ; mask off msb to max of 16KB
     116.  01:026C  E6 3F                   and     $3F
     117.  01:026E  F6 40                   or      $40                     ; set second highest bit to indicate write
     118.  01:0270  D3 99                   out     (VDP_PORT_REGS), a           ; send msb
     119.  01:0272  C9                      ret
