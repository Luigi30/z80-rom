       5.  01:0200                      VDP_B_ColdStart:
       6.  01:0200                          ; Copy the table from VDP_B_FnTable to the public table in RAM.
       7.  01:0200  01 00 01                ld  bc,256
       8.  01:0203  11 00 84                ld  de,VDP_FnTable_Public
