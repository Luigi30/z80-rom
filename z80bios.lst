      14.  01:020C                      ;;; ;;;;;;;;;;;;;;;;;
      15.  01:020C                      ;;; VDP functions
      16.  01:020C                      ;;;
      17.  01:020C                      ;;; API:
      18.  01:020C                      ;;;		All VDP BIOS functions are prefixed with VDP_B_
      19.  01:020C                      ;;; 	Input is A, BC, DE, HL
      20.  01:020C                      ;;;		Output bytes are in A
      21.  01:020C                      ;;;		Output words are in HL (todo: ?)
      22.  01:020C                      ;;;
      23.  01:020C                      ;;;		Do not assume any registers are preserved.
      24.  01:020C                      VDP_B_Dispatch:
      25.  01:020C                      	;; Dispatch to the function number C.
      26.  01:020C  D5                  	push	de
      27.  01:020D  C5                      push    bc
      28.  01:020E  F5                  	push	af
      29.  01:020F  E5                      push    hl
      30.  01:0210  21 00 84            	ld		hl,VDP_FnTable_Public	; grab the jump table address
      31.  01:0213  16 00               	ld		d,0		; clear D
      32.  01:0215                      	
      33.  01:0215  CB 21               	sla		c		; shift C to produce a table offset
      34.  01:0217  59                  	ld		e,c		; E <- C
      35.  01:0218  19                  	add		hl,de	; Apply the offset.
      36.  01:0219                      
      37.  01:0219  7E                  	ld		a,(hl)	; Get the destination address. 
      38.  01:021A  5F                  	ld		e,a
      39.  01:021B  23                  	inc		hl
      40.  01:021C  56                  	ld		d,(hl)
      41.  01:021D                      
      42.  01:021D  E5                  	push    hl
      43.  01:021E  DD E1                   pop     ix
      44.  01:0220                      
      45.  01:0220  E1                      pop     hl
      46.  01:0221  F1                  	pop		af		; Restore AF, DE, BC.
      47.  01:0222  C1                      pop     bc
      48.  01:0223  D1                  	pop		de
      49.  01:0224  DD E9               	jp		(ix)	; Jump to the BIOS function, which RETs back to where we started.
      50.  01:0226  C9                  	ret				; Unnecessary unless something breaks
      51.  01:0227                      
      52.  01:0227                      ; copy a null-terminated string to VRAM
      53.  01:0227                      ;       HL = ram source address
      54.  01:0227                      VDP_B_StringOut:
      55.  01:0227  7E                      ld      a, (hl)                 ; get the current byte from ram
      56.  01:0228  FE 00                   cp      0                       ; return when NULL is encountered
      57.  01:022A  C8                      ret     z
      58.  01:022B  D3 98                   out     (VDP_PORT_VRAM), a             ; send it to vram
      59.  01:022D  00 (3)                  defs    14/tmswait, 0         ; nops to waste time
      60.  01:0230  23                      inc     hl                      ; next byte
      61.  01:0231  18 F4                   jr      VDP_B_StringOut
      62.  01:0233                      
      63.  01:0233                      VDP_B_VRAMBlockCopy:
      64.  01:0233                      ; copy bytes from ram to vram
      65.  01:0233                      ;       HL = ram source address
      66.  01:0233                      ;       DE = vram destination address
      67.  01:0233                      ;       BC = byte count
      68.  01:0233  CD 68 02                call    VDP_B_SetVRAMAddress      ; set the starting address
      69.  01:0236                      .copyloop:
      70.  01:0236  7E                      ld      a, (hl)                 ; get the current byte from ram
      71.  01:0237  D3 98                   out     (VDP_PORT_VRAM), a           ; send it to vram
      72.  01:0239  00 (3)                  defs    14/tmswait, 0         ; nops to waste time
      73.  01:023C  23                      inc     hl                      ; next byte
      74.  01:023D  0B                      dec     bc                      ; continue until count is zero
      75.  01:023E  78                      ld      a, b
      76.  01:023F  B1                      or      c
      77.  01:0240  20 F4                   jr      nz, .copyloop
      78.  01:0242  C9                      ret
      79.  01:0243                      
      80.  01:0243                      VDP_B_SetRegister:
      81.  01:0243                          ; A is the value
      82.  01:0243                          ; E is the register number
      83.  01:0243                      
      84.  01:0243  21 72 81                ld      hl,VDP_ShadowRegs
      85.  01:0246  16 00                   ld      d,0
      86.  01:0248  19                      add     hl,de   ; Add offset to shadow register.
      87.  01:0249  77                      ld      (hl),a  ; Update shadow value.
      88.  01:024A  D3 99                   out     (VDP_PORT_REGS),a    ; send a byte of data
      89.  01:024C  3E 80                   ld      a,VDP_REG_WRITE ; select register to write it to
      90.  01:024E  B3                      or      e
      91.  01:024F  D3 99                   out     (VDP_PORT_REGS),a
      92.  01:0251                      
      93.  01:0251  C9                      ret
      94.  01:0252                      
      95.  01:0252                      VDP_B_SetRegistersFromArray:
      96.  01:0252                          ; HL is an array of registers.
      97.  01:0252  11 72 81                ld      de, VDP_ShadowRegs      ; start of shadow area
      98.  01:0255  0E 08               	ld      c, 8                    ; 8 registers
      99.  01:0257                      .regloop:
     100.  01:0257  7E                    	ld      a, (hl)                 ; get register value from table
     101.  01:0258  D3 99               	out     (VDP_PORT_REGS), a      ; send it to the TMS
     102.  01:025A  3E 08               	ld      a, 8                    ; calculate current register number
     103.  01:025C  91                  	sub     c
     104.  01:025D  F6 80               	or      VDP_REG_WRITE           ; set high bit to indicate a register
     105.  01:025F  ED A0                   ldi                             ; shadow, then inc pointers and dec counter
     106.  01:0261  D3 99               	out     (VDP_PORT_REGS), a           ; send it to the TMS
     107.  01:0263  AF                      xor     a                       ; continue until count reaches 0
     108.  01:0264  B1                      or      c
     109.  01:0265  20 F0               	jr      nz, .regloop
     110.  01:0267  C9                  	ret
     111.  01:0268                      
     112.  01:0268                      ; set the next address of vram to write
     113.  01:0268                      ;   DE = address
     114.  01:0268                      VDP_B_SetVRAMAddress:
     115.  01:0268  7B                      ld      a, e                    ; send lsb
     116.  01:0269  D3 99                   out     (VDP_PORT_REGS), a
     117.  01:026B  7A                      ld      a, d                    ; mask off msb to max of 16KB
     118.  01:026C  E6 3F                   and     $3F
     119.  01:026E  F6 40                   or      $40                     ; set second highest bit to indicate write
     120.  01:0270  D3 99                   out     (VDP_PORT_REGS), a           ; send msb
     121.  01:0272  C9                      ret
