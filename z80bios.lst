     123.  01:0273                      VDP_B_Reset:
     124.  01:0273                          ; Set up blank mode.
     125.  01:0273  21 ED 0A                ld      hl,VDP_DefaultRegisters
     126.  01:0276  CD 52 02                call    VDP_B_SetRegistersFromArray
     127.  01:0279                      
     128.  01:0279  11 00 00                ld      de,0
     129.  01:027C  CD 68 02                call    VDP_B_SetVRAMAddress
     130.  01:027F                      
     131.  01:027F  11 00 40                ld      de, $4000               ; write 16KB
     132.  01:0282  01 98 00                ld      bc, VDP_PORT_VRAM       ; writing 0s to vram
     133.  01:0285                      .clearloop:
     134.  01:0285  ED 41                   out     (c), b                  ; send to vram
     135.  01:0287  1B                      dec     de                      ; continue until counter is 0
     136.  01:0288  7A                      ld      a, d
     137.  01:0289  B3                      or      e
     138.  01:028A  20 F9                   jr      nz, .clearloop
     139.  01:028C                      
     140.  01:028C  C9                      ret
